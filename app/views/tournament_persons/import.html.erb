<%= bootstrap_form_for :persons, :url => url_for(:controller => 'tournament_person', :action => 'import_persons') do |f| %>
    <%= f.file_field :csv_file, :accept => 'text/csv', label: 'Список участников' %>
    <%= f.hidden_field :tournament_id, value:@tournament_id %>
    <%= f.submit 'Импорт' %>
<% end %>